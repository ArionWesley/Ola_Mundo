algoritmo "PrimeiraMatriz"
var
   M: Vetor[1..3, 1..3] de inteiro
   I, J: Inteiro
inicio
      Para I <- 1 ate 3 faca
           Para J <- 1 ate 3 faca
                Leia(M[I, J])
           Fimpara
      Fimpara
      Limpatela
      Para I <- 1 ate 3 faca
           Para J <- 1 ate 3 faca
                Escreva(M[I, J]: 3)
           Fimpara
           Escreval()
      Fimpara
fimalgoritmo
